{
  "name": "web-client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "antd": "^3.11.6",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "jwt-decode": "^2.2.0",
    "moment": "^2.23.0",
    "pdfmake": "^0.1.40",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-drag-listview": "^0.1.4",
    "react-moment": "^0.8.4",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "css-loader": "^2.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jsdoc-http-plugin": "^0.3.0",
    "jsdoc-to-markdown": "^4.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.7.0",
    "serve": "^10.1.1",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode=development",
    "dev": "webpack --watch --mode=development",
    "docs:client": "jsdoc2md -c .jsdoc -f src/**/* > client-docs/client.md",
    "docs:api": "jsdoc2md -c .jsdoc -f ./api/routes/**/*  > api-docs/api.md",
    "docs": "npm run docs:client && npm run docs:api",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:runInBand": "jest --runInBand --watchAll",
    "build": "webpack --mode=production",
    "babel": "babel ./src/App.jsx -d public",
    "serve": "npm run build && serve -l 80 public",
    "format": "prettier --write '**/*.{js,jsx}'",
    "lint": "eslint src/* --fix",
    "count": "find . -name '*.js' -not -path './node_modules*' -not -path './client/node_modules*' | xargs wc -l"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
